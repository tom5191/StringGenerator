{
  "version": "1.0.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "ts-string-generator",
  "author": "Thomas Horner",
  "module": "dist/ts-string-generator.esm.js",
  "size-limit": [
    {
      "path": "dist/ts-string-generator.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ts-string-generator.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "11.2.0",
    "husky": "9.1.7",
    "size-limit": "11.2.0",
    "tsdx": "0.14.1",
    "tslib": "2.8.1",
    "typescript": "3.9.10",
    "assert": "2.1.0"
  },
  "dependencies": {}
}
